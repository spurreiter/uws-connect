{
  "name": "uws-connect",
  "version": "1.2.0",
  "description": "Use connect like middlewares with uWebSockets.js",
  "keywords": [
    "uws",
    "uwebsocketsjs",
    "websockets",
    "connect",
    "express",
    "expressjs",
    "fast",
    "http-server",
    "https-server",
    "http",
    "https",
    "ws",
    "websocket",
    "performance",
    "router"
  ],
  "homepage": "https://github.com/spurreiter/uws-connect#readme",
  "bugs": {
    "url": "https://github.com/spurreiter/uws-connect/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/spurreiter/uws-connect.git"
  },
  "license": "MIT",
  "author": "spurreiter",
  "type": "module",
  "main": "src/index.js",
  "types": "types",
  "directories": {
    "test": "test"
  },
  "files": [
    "src",
    "types"
  ],
  "scripts": {
    "ci": "npm run lint && npm t && npm run types",
    "dev": "nodemon src/server.js",
    "lint": "eslint --ext .js --fix .",
    "start": "node src/server.js",
    "test": "c8 -r html -r text mocha",
    "types": "rm -r types; tsc && cp src/*.d.ts types"
  },
  "mocha": {
    "exit": true,
    "recursive": true
  },
  "dependencies": {
    "cookie": "^0.5.0",
    "uWebSockets.js": "git+https://github.com/uNetworking/uWebSockets.js.git#semver:^20.31.0"
  },
  "devDependencies": {
    "@types/cookie": "^0.5.2",
    "@types/node": "^20.6.5",
    "c8": "^8.0.1",
    "cors": "^2.8.5",
    "eslint": "^8.50.0",
    "eslint-config-standard": "^17.1.0",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-n": "^16.1.0",
    "eslint-plugin-promise": "^6.1.1",
    "mocha": "^10.2.0",
    "serve-static": "^1.15.0",
    "supertest": "^6.3.3",
    "typescript": "^5.2.2"
  },
  "engines": {
    "node": ">=16 <=20"
  }
}
